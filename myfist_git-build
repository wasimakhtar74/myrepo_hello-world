pipeline {
    agent any

    stages {
        stage('Test') {
            steps {
                echo 'Hello test'
                sleep 30
            }
        }
                stage('build') {
            steps {
                echo 'Hello build'
                sleep 30
            }
        }
                stage('preprod') {
            steps {
                echo 'Hello preprod'
                sleep 30
            }
        }
                stage('prod') {
            steps {
                echo 'Hello prod'
                sleep 30
            }
        }
    }
}
